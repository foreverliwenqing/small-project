.move_item {
  box-shadow: 0 5px 5px 0 rgba(0,0,0,.15);
  border-radius: 5px;
  background: #fff;
  font-size: 24rpx;
  overflow: hidden;
  width: 48%;
  margin: 10rpx 0;
}
.moveBox {
  justify-content: space-around;
  display: -webkit-flex; /* Safari */
  -webkit-flex-flow: row-reverse wrap; /* Safari 6.1+ */
  flex-flow: row-reverse wrap;
}
.swiper-tab {
  background: #f7f8fa;
}
.move_item .coverImg {
  width: 100%;
  position: relative;
}
.move_item .icon-bofang {
  position: absolute;
  left: 50%;
  top: 50%;
  font-size: 100rpx;
  color: black;
  opacity: .5;
  transform: translate(-50%, -50%);
}
.move_item .coverImg image{
  -o-object-fit: cover;
  object-fit: cover;
  height: 100%;
  width: 100%;
}
.move_item p {
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  margin: 16rpx;
}
.list_bottom {
  height: 100rpx;
  display: flex;
  align-items: center;
  padding: 0 10rpx 0;
}
.list_bottom .list_bottom_avatar {
  margin-right: 4rpx;
}
.list_bottom .list_bottom_avatar image {
  height: 50rpx;
  width: 50rpx;
  border-radius: 50%;
}
.list_bottom_left {
  display: flex;
  align-items: center;
  width: 62%;
}
.list_bottom .list_bottom_left span {
  width: 156rpx;
  text-overflow: -o-ellipsis-lastline;
  color: #888;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}
.list_bottom_right span {
  color:  #888;
  margin-left: 10rpx;
}
.list_bottom_right .iconfont{
  font-size: 24rpx;
}