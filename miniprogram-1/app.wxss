/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 100rpx 0;
  box-sizing: border-box;
} 
.nodata_img {
  width: 45px;
  height: 45px;
}
.nodata {
  height: calc(100vh - 120px);
  display: flex;
  justify-content: center;
  align-items: center;
}
.nodata_text {
  color: #dedede;
  font-size: 12px;
  padding-left: 10px;
}